model User {
  id String @id @default(cuid(2)) @db.VarChar(30)

  createdAt DateTime  @default(now()) @map("created_at")
  updatedAt DateTime  @default(now()) @updatedAt @map("updated_at")
  deletedAt DateTime? @map("deleted_at")

  lineUserId String @map("line_user_id")

  // 会社情報
  companyName              String? @map("company_name")
  companyAddressPrefecture String? @map("company_address_prefecture")
  companyAddressRest       String? @map("company_address_rest")
  industrialDivision       String? @map("industrial_division")
  industrialMajorGroup     String? @map("industrial_major_group")
  industrialGroup          String? @map("industrial_group")

  // 所属情報
  department String? @map("department")
  jobTitle   String? @map("job_title")

  // 個人情報
  name         String? @map("name")
  phoneticName String? @map("phonetic_name")
  phoneNumber  String? @map("phone_number")
  email        String? @map("email")

  sessions         Session[]
  userProfiles     UserProfile[]
  userProfileNotes UserProfileNote[]

  @@map("users")
}
