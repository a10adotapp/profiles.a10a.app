model UserProfileNote {
  id String @id @default(cuid(2)) @db.VarChar(30)

  createdAt DateTime  @default(now()) @map("created_at")
  updatedAt DateTime  @default(now()) @updatedAt @map("updated_at")
  deletedAt DateTime? @map("deleted_at")

  userId        String @map("user_id") @db.VarChar(30)
  profileId     String @map("profile_id") @db.VarChar(30)
  userProfileId String @map("user_profile_id") @db.VarChar(30)

  body String @map("body") @db.Text

  images Json @default("[]") @map("images")

  user        User        @relation(fields: [userId], references: [id])
  profile     Profile     @relation(fields: [profileId], references: [id])
  userProfile UserProfile @relation(fields: [userProfileId], references: [id])

  @@map("user_profile_notes")
}
